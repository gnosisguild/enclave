{
  "printWidth": 140,
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "semi": false,
  "trailingComma": "all",
  "arrowParens": "always",
  "overrides": [
    {
      "files": ["*.mdx", "*.md"],
      "options": {
        "printWidth": 100,
        "proseWrap": "always"
      }
    }
  ]
}
